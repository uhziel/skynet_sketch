    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch main_x51",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/skynet/skynet",
            "args": [
                "../netpack/config_x51"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/skynet",
            "environment": [],
            "MIMode": "lldb"
        },
        {
            "name": "(lldb) Launch lua",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/skynet/3rd/lua/lua",
            "args": [
                "-l",
                "hproto"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/netpack",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        },
        {
            "name": "clang++ build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "clang++ build active file"
        }
    ]
}